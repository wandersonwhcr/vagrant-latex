% article
% Artigo de Revista Científica ou Periódico
% Obrigatórios: author, title, journal, year
% Opcionais: volume, number, pages, month, note
@article{joao1993,
    author  = "João da Silva e Silva",
    title   = "O título do trabalho",
    journal = "O nome da revista científica",
    year    = 1993,
    number  = 2,
    pages   = "201-213",
    month   = 7,
    note    = "Uma nota opcional para a bibliografia",
    volume  = 4,
}

% book
% Livro Publicado
% Obrigatórios: author/editor, title, publisher, year
% Opcionais: volume/number, series, address, edition, month, note
@book{babington1993,
    author    = "Peter Babington",
    editor    = "O nome completo do editor",
    title     = "O título do trabalho",
    publisher = "O nome da publicadora",
    year      = 1993,
    volume    = 4,
    number    = 4,
    series    = 10,
    address   = "O endereço da publicadora",
    edition   = 3,
    month     = 7,
    note      = "Uma nota opcional para a bibliografia",
    isbn      = "1234567890",
}

% booklet
% Obra sem Vínculo com Publicadora ou Patrocinador
% Obrigatórios: title
% Opcionais: author, howpublished, address, month, year, note
@booklet{caxton1993,
    title        = "O título do trabalho",
    author       = "Peter Caxton",
    howpublished = "Como este livro foi publicado",
    address      = "O endereço da publicadora",
    month        = 7,
    year         = 1993,
    note         = "Uma nota opcional para a bibligrafia",
}

% conference
% Artigo em Conferência
% Obrigatórios: author, title, booktitle, year
% Opcionais: editor, volume/number, series, pages, address, month, organization, publisher, note
@conference{draper1993,
    author       = "Peter Draper",
    title        = "O título do trabalho",
    booktitle    = "O título do livro",
    year         = 1993,
    editor       = "O nome completo do editor",
    volume       = 4,
    series       = 5,
    address      = "O endereço da publicadora",
    month        = 7,
    organization = "O nome da organização",
    publisher    = "O nome da publicadora",
    note         = "Uma nota opcional para a bibliografia",
}
